{
  "version": 2,
  "builds": [
    {
      "src": "djangoSimpleBlog/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/djangoSimpleBlog/wsgi.py"
    }
  ],
  "env": {
    "PYTHONPATH": "/var/task",
    "DJANGO_SETTINGS_MODULE": "djangoSimpleBlog.settings",
    "ALLOWED_HOSTS": ".vercel.app"
  }
}